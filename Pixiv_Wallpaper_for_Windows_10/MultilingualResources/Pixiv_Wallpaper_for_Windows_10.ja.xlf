          <source>Local illustrations total size:</source>
          <target state="final">イラストファイルサイズ:</target>
        </trans-unit>
        <trans-unit id="Next.Text" translate="yes" xml:space="preserve">
          <source>Next illustration</source>
          <target state="final">次のイラスト</target>
        </trans-unit>
        <trans-unit id="NotSupport" translate="yes" xml:space="preserve">
          <source>Your device does not support automatic wallpaper replacement</source>
          <target state="final">お使いのデバイスは自動壁紙交換をサポートしていません</target>
        </trans-unit>
